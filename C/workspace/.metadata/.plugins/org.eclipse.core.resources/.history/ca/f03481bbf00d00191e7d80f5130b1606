#include "user_UART_related.h"

/* Exported functions definition*/
UART_RxCmdTypeDef UART_ParseRxCommand(unit8_t *UART_rxBuffer) {
	if (UART_rxBuffer[0] == 'L') {
		switch (UART_rxBuffer[1]) {
			case '0':
				return TURN_LED_OFF;
			case '1':
				return TURN_LED_ON;
			default:
				return INVALID;
		}
	}
	else return INVALID;
}
